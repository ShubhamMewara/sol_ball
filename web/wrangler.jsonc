{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "solball",
  "main": "server/index.ts",
  "compatibility_date": "2024-12-01",
  "compatibility_flags": ["nodejs_compat"],
  "durable_objects": {
    "bindings": [
      {
        "name": "Globe",
        "class_name": "Globe"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["Globe"]
    }
  ],
  "dev": {
    "port": 1999,
    "local_protocol": "http"
  }
}
